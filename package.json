{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate project for a webapp using React/Redux architecture.",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "CheesecakeLabs/react-redux-boilerplate",
  "scripts": {
    "start": "node dist-server/server.js",
    "dev": "NODE_ENV=development PORT=5000 node server.js",
    "build": "NODE_ENV=production npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --progress --colors --config webpack.config.production.js -p --bail",
    "build:server": "NODE_ENV=production webpack --progress --colors --config webpack.config.server.js --bail",
    "lint": "eslint src server"
  },
  "devEngines": {
    "node": "6.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
